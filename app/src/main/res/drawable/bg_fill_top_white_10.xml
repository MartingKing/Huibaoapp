<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 -->
    <solid android:color="@color/white" />
    <!-- 设置几何图形四个角的弧度 -->
    <corners
        android:topLeftRadius="10dp"
        android:topRightRadius="10dp"/>
    <!-- 设置几何形状绘制边框 -->
    <stroke
        android:color="@color/white"
        android:width="2px"/>
</shape>