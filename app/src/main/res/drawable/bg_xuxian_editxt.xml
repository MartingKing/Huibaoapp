<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 -->
    <solid android:color="@color/colorFAFAFA" />
    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="3dp" />
    <!-- 设置几何形状绘制边框 -->
    <stroke
        android:width="1dp"
        android:color="#8B8B8B"
        android:dashWidth="1dp"
        android:dashGap="2dp" />

</shape>